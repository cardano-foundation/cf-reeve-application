{
  "realm": "cf-master",
  "enabled": true,
  "authenticationFlows": [
    {
      "alias": "browser",
      "description": "Browser authentication flow with OTP required",
      "providerId": "basic-flow",
      "topLevel": true,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "auth-username-password-form",
          "priority": 1,
          "requirement": "REQUIRED",
          "userSetupAllowed": false
        },
        {
          "authenticator": "auth-otp-form",
          "priority": 2,
          "requirement": "REQUIRED",
          "userSetupAllowed": false
        }
      ]
    }
  ],
  "users": [
    {
      "username": "auditor",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "auditor",
            "temporary": true
        }
      ],
      "realmRoles": ["cf_auditor"]
    },
    {
      "username": "manager",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "manager",
          "temporary": true
        }
      ],
      "realmRoles": ["cf_manager"]
    }
  ],
  "clients": [
    {
      "clientId": "webclient",
      "enabled": true,
      "protocol": "openid-connect",
      "redirectUris": [
        "http://localhost:9000/*"
      ],
      "webOrigins": [
        "http://localhost:9000"
      ],
      "adminUrl": "http://localhost:9000",
      "bearerOnly": false,
      "consentRequired": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "publicClient": true,
      "surrogateAuthRequired": false
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "cf_manager",
        "description": "",
        "composite": false,
        "clientRole": false,
        "attributes": {}
      },
      {
        "name": "cf_auditor",
        "description": "",
        "composite": false,
        "clientRole": false,
        "attributes": {}
      }
    ]
  }
}